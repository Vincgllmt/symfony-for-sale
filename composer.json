{
    "name": "but/symfony-for-sale",
    "description": "Symfony 6.3",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "knplabs/knp-paginator-bundle": "^6.2",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.23",
        "stof/doctrine-extensions-bundle": "^1.8",
        "symfony/asset": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/doctrine-messenger": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/expression-language": "6.3.*",
        "symfony/flex": "^2",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/intl": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/mime": "6.3.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "6.3.*",
        "symfony/process": "6.3.*",
        "symfony/property-access": "6.3.*",
        "symfony/property-info": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/serializer": "6.3.*",
        "symfony/string": "6.3.*",
        "symfony/translation": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/web-link": "6.3.*",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "6.3.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpro/grumphp": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "symfony serve --no-tls"
        ],
        "fix:cs": [
            "php-cs-fixer fix"
        ],
        "test:cs": [
            "php-cs-fixer fix --dry-run"
        ],
        "test:twig": [
            "php bin/console lint:twig templates"
        ],
        "test:yaml": [
            "php bin/console lint:yaml config"
        ],
        "db": [
            "php bin/console doctrine:database:drop --force --if-exists",
            "php bin/console doctrine:database:create",
            "php bin/console doctrine:migrations:migrate --no-interaction",
            "php bin/console doctrine:fixtures:load --no-interaction"
        ],
        "db:test": [
            "php bin/console doctrine:database:drop --force --quiet --env=test",
            "php bin/console doctrine:database:create --quiet --env=test",
            "php bin/console doctrine:schema:create --quiet --env=test"
        ],
        "migrate": [
            "php bin/console make:migration"
        ],
        "test": [
            "@test:cs",
            "@test:twig",
            "@test:yaml",
            "@test:codeception"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test:codeception": [
            "@db:test",
            "php vendor/bin/codecept clean",
            "php vendor/bin/codecept run -g available",
            "php vendor/bin/codecept run -x available"
        ]
    },
    "scripts-descriptions": {
        "start": "Runs the symfony web server",
        "fix:cs": "Fixes the code style",
        "test:cs": "Checks the code style",
        "test:twig": "Checks that the Twig template syntax is correct",
        "test:yaml": "Checks that the YAML config files syntax is correct",
        "db": "Recreates the database",
        "migrate": "Creates a new migration based on database changes",
        "test:codeception": "Runs the codeception tests",
        "test": "Runs all the tests"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "codeception/module-doctrine2": "^3.0",
        "codeception/module-symfony": "^3.1",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsofphp/php-cs-fixer": "^3.25",
        "phpro/grumphp": "^2.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/debug-bundle": "6.3.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "6.3.*",
        "symfony/web-profiler-bundle": "6.3.*",
        "zenstruck/foundry": "^1.35",
        "codeception/module-phpbrowser": "*"
    }
}
